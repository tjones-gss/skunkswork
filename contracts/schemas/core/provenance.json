{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://nam-pipeline/contracts/schemas/core/provenance.json",
  "title": "Provenance",
  "description": "Tracks the origin and lineage of extracted data",
  "type": "object",
  "properties": {
    "source_url": {
      "type": "string",
      "format": "uri",
      "description": "URL where data was extracted from"
    },
    "source_type": {
      "type": "string",
      "enum": ["web", "api", "pdf", "manual"],
      "default": "web",
      "description": "Type of source"
    },
    "extracted_at": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp of extraction"
    },
    "extracted_by": {
      "type": "string",
      "description": "Agent that performed extraction"
    },
    "association_code": {
      "type": ["string", "null"],
      "description": "Association code if applicable"
    },
    "job_id": {
      "type": ["string", "null"],
      "description": "Pipeline job ID"
    },
    "page_type": {
      "type": ["string", "null"],
      "enum": [
        "ASSOCIATION_HOME",
        "ASSOCIATION_DIRECTORY",
        "MEMBER_DIRECTORY",
        "MEMBER_DETAIL",
        "EVENTS_LIST",
        "EVENT_DETAIL",
        "PARTICIPANTS_LIST",
        "EXHIBITORS_LIST",
        "SPONSORS_LIST",
        "RESOURCE",
        "OTHER",
        null
      ],
      "description": "Classified page type"
    },
    "confidence": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0,
      "default": 1.0,
      "description": "Extraction confidence"
    }
  },
  "required": ["source_url", "extracted_by"],
  "additionalProperties": false
}
